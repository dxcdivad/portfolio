        <div id="mongo" class="col-sm-2">
          <img src="https://s3-us-west-1.amazonaws.com/dh-portfolio-1/mongodb.png">
        </div>
        <div id="react" class="col-sm-2">
          <img src="https://s3-us-west-1.amazonaws.com/dh-portfolio-1/react.png">
